---
import type { CollectionEntry } from 'astro:content';
import Layout from './general.astro';

type MetaType = Partial<Omit<CollectionEntry<'blog'>['data'], 'publishedAt' | 'draft'>>;

export interface Props {
  meta: MetaType;
}

const { meta } = Astro.props;
---

<Layout meta={meta}>
  <slot />
</Layout>
