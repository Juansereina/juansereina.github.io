{
  "presets": [
    [ "es2015", { "modules": false } ],
    "react",
    "stage-2",
    "env"
  ],
  "env": {
    "targets": {
      "browsers": ["last 2 versions", "safari >= 7"],
      "node": "current"
    },
    "modules": false
  },
  "plugins": ["transform-runtime", "transform-class-properties"]
}
