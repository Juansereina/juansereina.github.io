{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/content/*": [
        "src/content/*"
      ],
      "@/data/*": [
        "src/data/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/utils": [
        "src/utils/index"
      ],
    }
  }
}
